    "@types/mime-types": "^2.1.1",
    "@types/qrcode": "^1.5.5",
    "date-and-time": "^3.0.1",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "replace": "^1.2.2",
    "svelte": "^3.47.0",
    "svelte-adapter-firebase": "^0.15.0",
    "svelte-check": "^3.4.6",
    "svelte-sequential-preprocessor": "^2.0.1",
    "tslib": "^2.4.1"
  },
  "type": "module",
  "dependencies": {
    "@egjs/svelte-grid": "^1.15.0",
    "@notionhq/client": "^2.2.14",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
