{
  "name": "skywalkerswebsite",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run config-dev && vite dev",
    "build": "npm run config-prod && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "firebase logout && firebase login && firebase deploy",
    "replace": "replace \"return !!\\(proto2 && proto2\\.constructor\\.name === \\\"AbortSignal\" \"return !!(proto2 && proto2.constructor.name === \\\"AbortSignal2\" build/functions/ssrServer/index.js && replace \"import_phone\\.default\" \"import_phone.phone\" build/functions/ssrServer/index.js",
    "config-prod": "replace 'csp_dev' 'csp_prod' svelte.config.js && replace 'client_secret_dev' 'client_secret_prod' src/lib/Google/client.ts",
    "config-dev": "replace 'csp_prod' 'csp_dev' svelte.config.js && replace 'client_secret_prod' 'client_secret_dev' src/lib/Google/client.ts",
    "serve": "npm run build && npm run replace && npm run deploy"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.1.2",
    "@melt-ui/svelte": "^0.55.3",
